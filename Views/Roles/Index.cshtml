@model IEnumerable<IdentityRole>

@{
    ViewBag.Title = "Roles";
}
<h1>Roles</h1>

<table class="table table-striped mt-4" style="color:white">
    <thead>
        <tr class = "bg-primary text-white">
            <td>Id</td>
            <td>Role Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>
